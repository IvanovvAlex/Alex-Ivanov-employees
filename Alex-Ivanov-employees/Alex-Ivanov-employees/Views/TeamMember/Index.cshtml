@model Alex_Ivanov_employees.Models.EmployeePair

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="file">Select a CSV file:</label>
        <input type="file" name="file" class="form-control-file" />
    </div>
    <button type="submit" class="btn btn-primary">Upload</button>
</form>

@if (Model == null)
{
    <p>No data available</p>
}
else
{
    <h3>The pair of employees who have worked together on common projects for the longest period of time are: @Model.FirstEmployeeID and @Model.SecondEmployeeID , for @Model.Duration days."</h3>
}
