@model IEnumerable<Alex_Ivanov_employees.Models.TeamMember.EmployeePair>


<form class="attach-file-form" method="post" enctype="multipart/form-data">
    <div id="form-elements" class="form-group">
        <label for="file">Select a CSV file:</label>
        <input type="file" name="file" class="form-control-file" />
    </div>
    <button id="sbm-btn" type="submit" class="btn btn-primary">Upload</button>
</form>

@if (Model != null)
{
    <h2>Longest Worked Projects:</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Employee 1</th>
                <th>Employee 2</th>
                <th>Project ID</th>
                <th>Duration</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.FirstEmployeeID</td>
                    <td>@item.SecondEmployeeID</td>
                    <td>@item.TeamID</td>
                    <td>@item.Duration</td>
                </tr>
            }
        </tbody>
    </table>
}
